<div class="notice_title">店舗matic</div>
<table width="100%">
  <col width="80%">
  <col width="20%">
  <tbody>
    <% @entries.each do |entry| -%>
      <tr>
        <td>
          <div class="contents_title"><%= entry[:status] %> <%= link_to(entry[:title], entry[:title_url]) %></div>
        </td>
        <td align="right">
          <span class="small_text" style="margin:3px;"><%= entry[:from] %></span>
        </td>
      </tr>
    <% end -%>
  </tbody>
</table>
